var mongoose = require('mongoose');
var Schema = mongoose.Schema;


var UserSchema = new Schema({
  name: {
    type: String,
    Required: 'Kindly enter the name of the task'
  },
  education: {
      type: String
  },
   city: {
      type: String
  },
   college: {
      type: String
  }
  
});

module.exports = mongoose.model('user', UserSchema);